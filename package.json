{
  "name": "notebook-chatcompletion",
  "displayName": "Jupyter Notebook ChatCompletion",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onNotebook:jupyter-notebook"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "notebook-chatcompletion.generateNewCodeCell",
        "title": "Generate new code cell via ChatCompletion"
      }
    ],
    "keybindings": [
      {
          "command": "notebook-chatcompletion.generateNewCodeCell",
          "key": "ctrl+shift+enter",
          "mac": "ctrl+cmd+enter"
      }
  ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.76.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "extensionDependencies": [
    "ms-toolsai.jupyter"
  ],
  "dependencies": {
    "highlight.js": "^11.7.0",
    "openai": "^3.2.1",
    "ws": "^8.13.0"
  }
}
